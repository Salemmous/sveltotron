{
	"name": "@sveltotron/debugger",
	"author": "Thomas De Bluts",
	"version": "1.0.0",
	"main": "src/electron.cjs",
	"private": true,
	"scripts": {
		"dev": "svelte-kit dev",
		"start": "svelte-kit start",
		"electron": "concurrently --kill-others 'svelte-kit dev -p 4000' 'electron src/electron.cjs'",
		"build": "svelte-kit build",
		"dev:package": "npm run build && electron-builder --config electron-builder.config.json --dir",
		"package": "npm run build && electron-builder --config electron-builder.config.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/socket.io": "^3.0.2",
		"autoprefixer": "^10.2.6",
		"concurrently": "^6.0.0",
		"cssnano": "^5.0.6",
		"electron": "^12.0.2",
		"electron-builder": "^22.10.5",
		"electron-reloader": "^1.2.0",
		"postcss": "^8.3.5",
		"postcss-load-config": "^3.1.0",
		"svelte": "^3.29.0",
		"svelte-preprocess": "^4.0.0",
		"tailwindcss": "^2.2.2",
		"vite": "^2.1.0"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^5.15.3",
		"electron-serve": "^1.1.0",
		"electron-window-state": "^5.0.3",
		"socket.io": "^4.1.2",
		"svelte-awesome": "^2.3.2"
	}
}
